            <GridLayout height="50" columns="*, auto">
                <Label verticalAlignment="center" text="{{ room }}"/>
                <Label col="1" verticalAlignment="center" text="{{ ratio }}"/>
            </GridLayout>
        </ListView.itemTemplate>
    </ListView>
</Page>
